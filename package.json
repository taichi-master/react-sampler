{
  "name": "react-sampler",
  "version": "1.0.0",
  "description": "React Sampler",
  "main": "index.js",
  "cfg": {
    "name": "React",
    "menuItems": [
      {
        "name": "Home",
        "url": "/"
      }
    ],
    "initialState": {
      "userId": 1
    }
  },
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js ./src ./server",
    "test": "jest",
    "dev": "NODE_ENV=development PORT=3000 nodemon -w server server",
    "build": "webpack --env.build",
    "dist": "webpack --env.dist",
    "start": "node server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taichi-master/react-sampler.git"
  },
  "keywords": [
    "isomorphic",
    "react",
    "redux",
    "redux-saga",
    "react-router",
    "sass",
    "webpack",
    "webpack-dev-middleware",
    "webpack-hot-middleware",
    "express",
    "ssl",
    "https",
    "es6",
    "babel",
    "javascript",
    "node.js",
    "REST",
    "hot-reload",
    "jest"
  ],
  "author": "Kei Sing Wong",
  "license": "UNLICENSED",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "server/**/*.{js,jsx}",
      "!server/libs/**",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "setupFiles": [
      "<rootDir>/src/setupTests.js"
    ], 
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "package.json": "<rootDir>/package.json",
      "@src(.*)$": "<rootDir>/src/$1",
      "@models(.*)$": "<rootDir>/src/models/$1",
      "@views(.*)$": "<rootDir>/src/views/$1",
      "@components(.*)$": "<rootDir>/src/views/components/$1"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@hot-loader/react-dom": "^16.8.6",
    "acorn": "^6.1.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.10.0",
    "nodemon": "^1.18.11",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "react-hot-loader": "^4.8.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "express": "^4.16.4",
    "morgan": "^1.9.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.0.2",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
    "serve-favicon": "^2.5.0",
    "styled-components": "^4.2.0"
  }
}
